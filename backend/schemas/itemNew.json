{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/itemNew.json",
  "type": "object",
  "properties": {
    "username": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    },

    "itemname": {
      "type": "string",
      "minLength": 1,
      "maxLength": 30
    }
  },
  "additionalProperties": false,
  "required": ["username", "itemname"]
}
